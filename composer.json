
{
    "name": "maxviex/tuya-laravel",
    "description": "Laravel connector untuk Tuya IoT API",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Your Name",
            "email": "your-email@example.com"
        }
    ],
    "require": {
        "php": "^8.0",
        "illuminate/support": "^8.0|^9.0|^10.0|^11.0|^12.0",
        "illuminate/http": "^8.0|^9.0|^10.0|^11.0|^12.0",
        "guzzlehttp/guzzle": "^7.0"
    },
    "autoload": {
        "psr-4": {
            "maxviex\\TuyaLaravel\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "maxviex\\TuyaLaravel\\TuyaServiceProvider"
            ],
            "aliases": {
                "Tuya": "maxviex\\TuyaLaravel\\Facades\\Tuya"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}